extends layout

block content
	form(method='post')
		h1 用户详情

		p 用户名: 
			span #{user.username}

		p 学号: 
			span #{user.sid}

		p 电话: 
			span #{user.phone}

		p 邮箱: 
			span #{user.email}

		.buttons
			input.button.submit(type='submit' id='reset' value='退出')

	#wrongSignin
		p=wrong.temp